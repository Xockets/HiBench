
# override configurations here
hibench.default.map.parallelism		1
hibench.default.shuffle.parallelism 	1
hibench.scale.profile			tiny
hibench.workload.input			${hibench.wordcount.base_hdfs}/${hibench.workload.dir.name.input}.x_2_1
hibench.workload.output			${hibench.wordcount.base_hdfs}/${hibench.workload.dir.name.output}.x_2_1

# Don't combine any words for keys
hibench.randomtextwriter.minwordskey	0
hibench.randomtextwriter.maxwordskey	0

